<l:HorizontalLayout
	allowWrapping="true"
	content="{/producers}">
	<core:Fragment fragmentName="com.rtdi.bigdata.connector.ui.fragment.xml.producer" type="XML" />
</l:HorizontalLayout>

<f:Card
	class="sapUiMediumMargin"
	width="200px" 
	enabled="{path: 'state>/edit', formatter: '.enableControl'}">
	<f:header>
		<card:Header
			title="Add new"
			iconSrc="sap-icon://add"
			press="onTileAdd" />
	</f:header>
</f:Card>
