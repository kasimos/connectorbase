<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:card="sap.f.cards"
    >
	<f:Card
		class="sapUiMediumMargin"
		width="200px">
		<f:header>
			<card:Header
				title="{consumername}"
				subtitle="{instancecount}"
				iconSrc="sap-icon://detail-view"
				press="onPressConsumer" />
		</f:header>
	</f:Card>
</core:FragmentDefinition>